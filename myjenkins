pipeline {
    agent any

    stages {
        stage('start') {
            steps {
                echo 'Starting project'
            }
        }
        stage('build') {
            steps {
                echo 'building project'
            }
        }
        stage('test') {
            steps {
                echo 'testing stage'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploying stage'
            }
        }
    }
}
