pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('build') {
            steps {
                echo 'building project'
            }
        }
        stage('test') {
            steps {
                echo 'testing stage'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploying stage'
            }
        }
    }
}
